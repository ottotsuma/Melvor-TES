{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": false,
    "noEmitOnError": false
  },
  "include": [
    "src/**/*",
    "node_modules/melvor-types",
    "_cmimDefinitions/**/*",
    "../Custom-Modifiers-in-Melvor/_definitions/**/*"
  ]
}
